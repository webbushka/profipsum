<section class="container-fluid">
	<h1>Profipsum</h1>
	<h4>your profile content ipsum</h4>
	<form class="ipsum-form">
		<input placeholder="how many profiles you want?" class="qty" type="number">
		<button class="btn btn-primary generate-btn">Generate</button>
	</form>
	{{#error}}
		<div class="alert alert-error">
			<strong>{{error}}</strong>
		</div>
	{{/error}}
	{{#items.0}}
		<div class="success alert alert-success">
			<strong>{{batfight}}</strong>
		</div>
	<div class="results well text-left row-fluid">
		{{#items}}
			<p class="span4">{{firstName}} {{lastName}}<br>{{eprefix}}{{firstNameLower}}{{esuffix}}@{{emailClient}}.com</p>
		{{/items}}
	</div>
	{{/items.0}}
</section>
<footer>
		&nbsp;
</footer>